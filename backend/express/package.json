{
  "name": "influx-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "workspaces": [
    "InfluxDatabase",
    "db-benchmark"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsoa spec-and-routes && tsc",
    "local": "ts-node ./src/server.ts",
    "start": "npm run build && ts-node ./src/server.ts",
    "dev": "nodemon",
    "run": "node dist/express/src/server.js",
    "stage": "npm run build && ts-node ./src/server.ts",
    "generate": "npm run build && ng-openapi-gen --input src/swagger.json --output ../../frontend/src/app/generated",
    "schema": "typescript-json-schema \"../../frontend/src/app/generated/models/*.ts\" \"*\" --defaultProps true --noExtraProps true --required true --id Api -o \"../influx-lambda/src/Api.json\"",
    "generate-next": "tsoa spec-and-routes && openapi --input src/swagger.json --output ../../generator/express-api",
    "generate-axios": "tsoa spec-and-routes && openapi --input src/swagger.json --output generated --client axios --useUnionTypes"
  },
  "keywords": ["IoT", "Time Series", "AWS", "Influx"],
  "author": "Petr John",
  "license": "MIT",
  "bugs": {
	"url": "https://github.com/pocketix/pocketix-node-core/issues"
  },
  "homepage": "https://github.com/pocketix/pocketix-node-core#readme",
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "codelyzer": "^6.0.2",
    "eslint": "^8.8.0",
    "jest": "^27.5.1",
    "multer": "^1.4.4",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typeorm": "^0.3.5",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@fullerstack/nax-ipware": "^0.10.0",
    "@influxdata/influxdb-client": "^1.20.0",
    "@influxdata/influxdb-client-apis": "^1.18.0",
    "@pocketix/pocketix-node": "github:pocketix/pocketix-node",
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "dip": "file:../..",
    "dotenv": "^8.6.0",
    "express": "^4.17.2",
    "express-requests-logger": "^4.0.0",
    "express-validator": "^6.14.0",
    "mongodb": "^3.7.3",
    "mysql": "^2.18.1",
    "openapi-typescript-codegen": "^0.23.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "swagger-express-ts": "^1.1.0",
    "swagger-ui-dist": "^4.5.0",
    "ts-json-schema-generator": "^0.98.0",
    "tsoa": "^4.0.0-beta.0",
    "typedi": "^0.10.0",
    "typeorm-naming-strategies": "^4.1.0"
  }
}
